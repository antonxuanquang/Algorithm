
public class Seven {
	public static void main (String[] args) {
		int matrix[][] = {{2,3,7,6},
		                  {1,1,6,7},
		                  {9,0,5,1},
		                  {8,9,10,11}};
		printMatrix(matrix);
		rotateMatrix(matrix);
	}

	private static String rotateMatrix(int[][] matrix) {
		return matrix.toString();
	}
}
