
public class BinarySearchTree {
	private class Node {
		Node left;
		Node right;
		
		public void setLeft(Node item) {
			this.left = item; 
		}

		public Node getLeft() {
			return this.left; 
		}

		public void setRight(Node item) {
			this.right = item; 
		}
		
		public void getRight() {
			return this.right; 
		}
	}
	
	Node root;
	
	public BinarySearchTree() {
		root = new Node();
	}
	
	
}
