package two.secondApproach;

public abstract class Employee {
	
	protected String name;
	protected Rank rank;
	protected Employee nextEmployee;
	
	public void dispatchCall(Call call, CallPool pool) {
		if (rank.getRank() >= call.getRank()) {
			handleCall(call);
		} else {
			nextEmployee.handleCall(call);
		}
	}
	
	abstract void handleCall(Call call);
}
