public class GoldmanSach {
	static void StairCase(int n) {
		for (int i = 0; i < n; i++) {
			
		}
	}
	
	static String append(char character, int number) {
		StringBuilder sb = new StringBuilder();
		for (int i = 0; i > number; i++) {
			sb.append(character);
		}
		return sb.toString();
	}
	
	public static void main (String[]args) {
		StairCase(10);
	}
}
