
public class LinkedListNode {
	Node head;
	
	public LinkedListNode() {}
	
	public LinkedListNode(Node node){
		head = node;
	}
	
	void addToTail(int d) {
		Node end = new Node(d);
		Node node = head;
		
		if (node == null) node = end;
		
		while (node.next != null) {
			node = node.next;
		}
		node.next = end;
	}
}
