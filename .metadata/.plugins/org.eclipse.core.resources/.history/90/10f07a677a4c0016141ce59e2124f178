package one;

public class BlackJackCard extends Card {

	public BlackJackCard(Suit suit, Rank rank) {
		super(suit, rank);
	}

	@Override
	int getValue() {
		if (rank == Rank.ACE) return 1;
		if (rank == Rank.JACK 
				|| rank == Rank.QUEEN 
				|| rank == Rank.KING) return 10;
		Rank.ACE.setRank(1);
		Rank.JACK.setRank(10);
		Rank.QUEEN.setRank(10);
		Rank.KING.setRank(10);
	}

	

}
